<?xml version="1.0" encoding="UTF-8"?>
<manifest>

  <!--Remote-->
  <remote  name="github-private"
           fetch="ssh://git@github.com" />
  <remote  name="gitlab"
           fetch="https://gitlab.com" />

  <!--Device repos--> 
  <project path="device/xiaomi/surya" name="JarlPenguin/device_xiaomi_surya" remote="github" revision="thirteen" />
  <project path="device/xiaomi/surya-miuicamera" name="JarlPenguin/device_xiaomi_surya-miuicamera" remote="github" revision="thirteen" />
  <project path="hardware/xiaomi" name="LineageOS/android_hardware_xiaomi" remote="github" revision="lineage-20" />
  <project path="kernel/xiaomi/surya" name="JarlPenguin/kernel_xiaomi_surya" remote="github" revision="thirteen" />
  <project path="vendor/xiaomi/surya" name="JarlPenguin/vendor_xiaomi_surya" remote="github-private" revision="thirteen" />
  <project path="vendor/xiaomi/surya-miuicamera" name="chrisw444/vendor_xiaomi_surya-miuicamera" remote="gitlab" revision="thirteen" />

  <!--Forks-->
  <remove-project name="bionic" />
  <project path="bionic" name="JarlPenguin/bionic" remote="github" revision="thirteen" />
  <remove-project name="build_soong" />
  <project path="build/soong" name="JarlPenguin/build_soong" remote="github" revision="thirteen" >
    <linkfile src="root.bp" dest="Android.bp" />
    <linkfile src="bootstrap.bash" dest="bootstrap.bash" />
  </project>
  <remove-project name="frameworks_base" />
  <project path="frameworks/base" name="JarlPenguin/frameworks_base" remote="github" revision="thirteen" />
  <remove-project name="frameworks_av" />
  <project path="frameworks/av" name="JarlPenguin/frameworks_av" remote="github" revision="thirteen" />
  <remove-project name="vendor_aosp" />
  <project path="vendor/aosp" name="JarlPenguin/vendor_aosp" remote="github" revision="thirteen" />
  <remove-project name="vendor_gms" />
  <project path="vendor/gms" name="JarlPenguin/vendor_gms" remote="gitlab" revision="thirteen" />
    
  <!--Extra-->
  <project path="vendor/certs" name="JarlPenguin/android_vendor_certs" remote="github-private" revision="main" />
  <project path="vendor/extra" name="JarlPenguin/android_vendor_extra" remote="github" revision="thirteen" />

</manifest>
